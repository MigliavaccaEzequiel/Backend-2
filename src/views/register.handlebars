<h1>{{title}}</h1>

<form action="/api/sessions/register" method="post">
  <div>
    <div>
      <label for="first_name">Nombre:</label>
      <br />
      <input type="text" id="first_name" name="first_name" placeholder="Nombre" />
    </div>
    <div>
      <label for="last_name">Apellido:</label>
      <br />
      <input type="text" id="last_name" name="last_name" placeholder="Apellido" />
    </div>
    <div>
      <label for="email">Email:</label>
      <br />
      <input
        type="email"
        id="email"
        name="email"
        placeholder="tu-mail@mail.com"
      />
    </div>
    <div>
      <label for="age">Edad:</label>
      <br />
      <input type="number" id="age" name="age" placeholder="Edad (número)" />
    </div>
    <div>
      <label for="password">Password:</label>
      <br />
      <input type="password" id="password" name="password" placeholder="****" />
    </div>
    <br />
    <button type="submit"> Registrar</button>
  </div>
</form>
